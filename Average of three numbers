#include<stdio.h>
int main()
{
    int a,b,c;
    float d;
    printf("Enter your first number");
    scanf("%d",&a);
    printf("Enter your second number");
    scanf("%d",&b);
    printf("Enter your third number");
    scanf("%d",&c);
    d=(a+b+c)/3;
    printf("The average of three numbers is %.2f",d);
    return 0;
}    
